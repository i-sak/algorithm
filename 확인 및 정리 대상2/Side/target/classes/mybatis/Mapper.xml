<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mb_map">
	
	<insert id="insert" parameterType="dateVo">
		INSERT INTO test_tb 
		(d1, d2, id) VALUES ( #{d1}, #{d2}, #{id} )	
	</insert>

	<select id="select" resultType="dateVo">
		SELECT * FROM test_tb WHERE id = 2
	</select>

</mapper>
